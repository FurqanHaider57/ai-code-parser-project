
{
  "mathematical_functions": {
    "factorial": {
      "preconditions": [
        "requires n >= 0;",
        "requires n <= 20;"
      ],
      "postconditions": [
        "ensures \\result >= 1;",
        "ensures n == 0 ==> \\result == 1;",
        "ensures n > 0 ==> \\result == n * \\old(factorial(n-1));"
      ]
    },
    "fibonacci": {
      "preconditions": [
        "requires n >= 0;",
        "requires n <= 45;"
      ],
      "postconditions": [
        "ensures \\result >= 0;",
        "ensures n <= 1 ==> \\result == n;",
        "ensures n > 1 ==> \\result == \\old(fibonacci(n-1)) + \\old(fibonacci(n-2));"
      ]
    }
  },
  "control_systems": {
    "pid_controller": {
      "preconditions": [
        "requires \\is_finite(setpoint) && \\is_finite(measurement);",
        "requires kp >= 0.0 && ki >= 0.0 && kd >= 0.0;",
        "requires kp + ki + kd > 0.0;"
      ],
      "postconditions": [
        "ensures \\is_finite(\\result);",
        "ensures setpoint == measurement ==> \\abs(\\result) <= 0.1;"
      ]
    }
  },
  "memory_management": {
    "safe_malloc": {
      "preconditions": [
        "requires size > 0;",
        "requires size <= SIZE_MAX;"
      ],
      "postconditions": [
        "ensures \\result == \\null || \\valid(\\result);",
        "ensures \\result != \\null ==> \\fresh(\\result, size);"
      ]
    }
  },
  "signal_processing": {
    "digital_filter": {
      "preconditions": [
        "requires \\is_finite(input);",
        "requires \\valid_read(coeffs, order);",
        "requires order > 0;"
      ],
      "postconditions": [
        "ensures \\is_finite(\\result);",
        "ensures \\abs(input) <= 1.0 ==> \\abs(\\result) <= 10.0;"
      ]
    }
  }
}
